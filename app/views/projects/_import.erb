<li id="req_<%= import[:id] %>">
<% if not Project.find_by_name(import[:project_name]) %>
<strong>new project</strong>
<% end %>
<% if not Project.find_by_name(get_workpackage_name_from_summary(import[:summary], import[:project_name])) %>
