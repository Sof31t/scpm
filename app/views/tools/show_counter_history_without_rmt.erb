<!-- WITHOUT REQUESTS -->
<h2>Spider Counters not linked with request</h2>
<table class="history_counts_table" >
<tr class="bg_grey"><td>Author</td><td>Action date</td><td>Project</td></tr>
<% @spider_counter_no_request.each do |sc_no_req| %>
	<tr>
		<td><%= sc_no_req.author.name %></td>
		<td><%= sc_no_req.action_date.strftime("%m/%d/%Y %H:%M") %></td>
		<td><%= link_to(sc_no_req.spider.project.name, {:controller=>'projects', :action=>'show', :id=>sc_no_req.spider.project.id}) %></td>
	</tr>
<% end %>
</table>

<h2>SQ Counters not linked with request</h2>
<table class="history_counts_table" >
<tr class="bg_grey"><td>Author</td><td>Action date</td><td>Project</td><td>Color</td></tr>
<% @qs_counter_no_request.each do |qsc_no_req| %>
	<tr>
		<td><%= qsc_no_req.author.name %></td>
		<td><%= qsc_no_req.action_date.strftime("%m/%d/%Y %H:%M") %></td>
		<td>
			<%= link_to(qsc_no_req.status.project.name, {:controller=>'projects', :action=>'show', :id=>qsc_no_req.status.project.id}) %>
		</td>
		<td><%=html_status(qsc_no_req.status.status)%></td>	</tr>
<% end %>
</table>